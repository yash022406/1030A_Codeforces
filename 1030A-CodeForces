#include<stdio.h>
int main(){
    int n,flag=0;
    scanf("%d",&n);
    while(n--){
        int a;
        scanf("%d",&a);
        if(a==1){
            printf("HARD");
            return 0;
        }
        else{
                flag=1;
        }
    }
    if(flag==1){
        printf("EASY");
    }
    return 0;
}